{
 "forward": {
   "timeout_sec": 30,
   "headers": {
     "Accept": "application/json"
   }
 },
 "mapping": {
   "source_root": null,
   "fields": {
     "name":         { "from": "full_name" },
     "email":        { "from": "email" },
     "phone_number": { "from": "phone_number" },
     "birth_date":   { "from": "birth_date" },
     "request_id":   { "from": "request_id" }
   },
   "answers_path": "questionAnswers",
   "answer_id_key": "question",
   "answer_value_key": "answer"
 },
 "response": {
   "type": "json",
   "xml_root": "BackendResponse"
 }
}
