{
 "forward": {
   "timeout_sec": 30,
   "headers": {
     "Accept": "application/json"
   }
 },
 "mapping": {
   "source_root": null,
   "answers_path": "answers",
   "answer_id_key": "id",
   "answer_value_key": "value",
   "fields": {
     "customer_name": { "from": "customer.name" },
     "mobile":        { "from": "customer.mobile" },
     "session_id":    { "from": "sessionId" },
     "style":         { "from": "answers.q1_style" },
     "metal":         { "from": "answers.q2_metal", "transform": "lower" },
     "budget":        { "from": "answers.q3_budget", "transform": "int" },
     "ring_size":     { "from": "answers.q4_size" },
     "profile":       { "from": "answers.q5_profile" },
     "notes":         { "from": "notes", "default": "" }
   },
   "wrap": {
     "customer":  ["customer_name", "mobile"],
     "preferences": ["style", "metal", "budget", "ring_size", "profile"]
   },
   "context": {
     "channel": "studio",
     "source": "FYRS"
   }
 },
 "response": {
   "type": "json",
   "xml_root": "BackendResponse"
 }
}
